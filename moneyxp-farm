local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mad cityBy Tiger", "DarkTheme")
local Tab = Window:NewTab("Mad city farm")

local Section = Tab:NewSection("Money&XP")
Section:NewButton("AUTO Money", "ButtonInfo", function()
    loadstring(game:HttpGet('https://system-exodus.com/scripts/madcity/MadLadsAR.lua',true))()
end)
Section:NewButton("AUTO XP", "ButtonInfo", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Cesare0328/my-scripts/main/XP-FARM-1MIN.lua', true))()
end)
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("WhiteList#1853")
local Section = Tab:NewSection("IM THAILAND")
local Section = Tab:NewSection("THX u for use")
